{
  "name": "pvsaintpe/yii2-log",
  "description": "Package",
  "keywords": [
    "yii2",
    "gii",
    "code generator"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Veselov",
      "email": "pvsaintpe@icloud.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "2.0.*",
    "yiisoft/yii2-bootstrap": "2.*",
    "yiisoft/yii2-swiftmailer": "2.*",
    "yiisoft/yii2-imagine": "2.*",
    "yiisoft/yii2-authclient": "2.*",
    "pvsaintpe/yii2-db-plus": "*",
    "pvsaintpe/yii2-search": "3.*",
    "pvsaintpe/yii2-behaviors": "*",
    "yiisoft/yii2-debug": "2.*",
    "kartik-v/yii2-editable": "1.*",
    "kartik-v/yii2-datecontrol": "1.*",
    "kartik-v/yii2-date-range": "1.*",
    "kartik-v/yii2-field-range": "1.*",
    "kartik-v/yii2-tabs-x": "1.*",
    "kartik-v/yii2-widgets": "3.*",
    "kartik-v/yii2-icons": "1.*",
    "kartik-v/yii2-dropdown-x": "*",
    "kartik-v/yii2-mpdf": "v1.*",
    "codeception/codeception": "2.*"
  },
  "autoload": {
    "psr-4": {
      "pvsaintpe\\log\\": ""
    }
  },
  "extra": {
    "bootstrap": "pvsaintpe\\log\\Bootstrap"
  },
  "config": {
    "fxp-asset.installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
    }
  }
}
